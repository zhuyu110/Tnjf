.count .daynumber {
  display: flex;
  flex-direction: row;
  justify-content: center;
}
.count {
  margin: 0rpx;
  padding: 0rpx;
  display: flex;
  text-align: center;
  border-radius: 10px;
  flex-direction: column;
  justify-content: center;
  background-color: #fff;
  align-items: center;
}
.count .number {
  color: #fff;
  font-size: 60rpx;
  background-color: #94db98;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin: 20rpx;
}
.monthSum {
  color: red;
  font-size: 40rpx;
}
